  "devDependencies": {
    "@-package-/eslint-config": "0.2.0",
    "@-package-/prettier-config": "^0.1.0",
    "@-package-/tsconfig": "^0.1.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.3.2"
  },
  "eslintConfig": {
    "extends": [
      "@-package-/eslint-config/base"
    ]
